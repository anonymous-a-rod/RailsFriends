<tr id="<%= dom_id employee %>">
  <td>
    <%= employee.first_name %>
  </td>
  <td>
    <%= employee.last_name %>
  </td>
  <td>
    <%= employee.email %>
  </td>
  <td>
    <%= employee.phone %>
  </td>
  <td>
    <%= link_to "Show", employee_path(employee), class: 'btn btn-primary' %>
  </td>
  <td>
    <%= link_to "Edit", edit_employee_path(employee), class: 'btn btn-secondary' %>
  </td>
  <td>
    <%= button_to "Destroy this employee", employee, method: :delete, class: 'btn btn-danger' %>
  </td>
  <div>
  </div>
</tr>
